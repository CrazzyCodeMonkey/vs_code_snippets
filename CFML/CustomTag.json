{
	"CFML Custom Tag:: Self Closing": {
		"prefix": "cftagsc",
		"description": "A Self Closing CFML Custom Tag with supporting Dragon Docs",
		"body": [
			"<cfsilent>",
			"\t<!---",
			"\t\t/**",
			"\t\t* ${1:Tag_Description}",
			"\t\t*   !This is a self-closing tag!",
			"\t\t*",
			"\t\t* @tag ${2:${TM_FILENAME/(.*)\\..+$/$1/}}",
			"\t\t* @throws CustomTag.INVALID_USEAGE",
			"\t\t* @example",
			"\t\t*   <cfimport taglib=\"${3:${TM_DIRECTORY}}\" prefix=\"${4:Tag_Prefix}\" />",
			"\t\t*   <${4:Tag_Prefix}:${2:${TM_FILENAME/(.*)\\..+$/$1/}} />",
			"\t\t*/",
			"\t--->",
			"\t<cfif (thisTag.executionMode == \"start\")>",
			"\t\t<cfscript>",
			"\t\t\t//define constants",
			"\t\t\tTAG_PATH = \"${3:TM_DIRECTORY}/${2:${TM_FILENAME/(.*)\\..+$/$1/}}\";",
			"\t\t\tTAG_NAME = listLast(TAG_PATH,\"/\");",
			"\t\t\tERRORS = {",
			"\t\t\t\tINVALID_USEAGE: \"Invalid Tag Usage: #TAG_NAME#\"",
			"\t\t\t\t//Add custome Error messages here",
			"\t\t\t};",
			"",
			"\t\t\t//Param all attributes",
			"\t\t\t//param name=\"attributes.name\" type=\"string\" default=\"\";  <-- This is a optional attribute ",
			"\t\t\t//param name=\"attributes.name\" type=\"string\";  <-- This is a required attribute ",
			"",
			"\t\t\t//Tag Usage Validation",
			"\t\t\tif (thisTag.hasEndTag){",
			"\t\t\t\tthrow(\"CustomTag\", ERRORS.INVALID_USEAGE, \"This tag only supports self-closing usage\");",
			"\t\t\t}",
			"",
			"\t\t\t//Attribute Validation",
			"\t\t\t//throw(TAG_NAME, ERRORS.CUSTOM_ERROR_STRING, \"put useful description in here\"); <-- example throw statement",
			"",
			"\t\t</cfscript>",
			"\t<cfelseif (thisTag.executionMode==\"end\")>",
			"\t\t<!--- the END mode is called immediately after the start despite no explicit closed tag --->",
			"\t\t<!--- No extra validation or work should be done here --->",
			"\t\t<cfsavecontent variable=\"thisTag.generatedContent\" trim=\"true\"><cfoutput>",
			"<!--- ****************************************************************** --->",
			"${5:Content_To_Generate}",
			"<!--- ****************************************************************** --->",
			"\t\t</cfoutput></cfsavecontent>",
			"\t</cfif>",
			"</cfsilent>"
		]
	},
